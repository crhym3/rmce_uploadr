<% @images.each do |image| %>
  <div class="image">
    <img src="<%= image.data.url(:thumb) %>" width="100" height="100" class="srcimage"
         data-src-original="<%= image.data.url(:original) %>" />
  </div>
<% end %>

<% if @images.empty? %>
  <em class="quiet">No images found, yet.</em>
<% end %>